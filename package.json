{
  "name": "project-maestro",
  "version": "0.1.0",
  "description": "A communication-centric code generation environment with AI agent collaboration, comprehensive testing infrastructure, and domain-driven architecture",
  "main": "dist/main/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && vite build && electron-builder",
    "preview": "vite preview",
    "start": "electron .",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --fix",
    "lint:check": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:contracts": "vitest run tests/contracts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:electron": "playwright test --project=electron",
    "test:e2e:web": "playwright test --project=chromium",
    "test:claude-generate": "ts-node tests/claude-driven/test-generator.ts",
    "test:all": "npm run test:run && npm run test:e2e",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:component": "node scripts/generate-component.js",
    "generate:service": "node scripts/generate-service.js",
    "validate:architecture": "node scripts/validate-architecture.js",
    "prepare": "husky install",
    "git:commit": "node scripts/github-workflow.cjs commit",
    "git:progress": "node scripts/github-workflow.cjs progress",
    "git:feature": "node scripts/github-workflow.cjs feature",
    "git:complete": "node scripts/github-workflow.cjs complete",
    "git:fix": "node scripts/github-workflow.cjs fix",
    "git:docs": "node scripts/github-workflow.cjs docs",
    "git:push": "node scripts/github-workflow.cjs push",
    "git:pr": "node scripts/github-workflow.cjs pr",
    "git:daily": "node scripts/github-workflow.cjs daily",
    "git:status": "node scripts/github-workflow.cjs status",
    "setup:github-mcp": "bash scripts/setup-github-mcp.sh",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "ai",
    "code-generation"
  ],
  "author": "Project Maestro Team",
  "license": "MIT",
  "devDependencies": {
    "@eslint/config-array": "^0.18.0",
    "@eslint/object-schema": "^2.1.4",
    "@playwright/test": "^1.53.1",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-storybook": "^0.10.2",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "playwright": "^1.53.1",
    "postcss": "^8.5.1",
    "prettier": "^3.3.3",
    "storybook": "^8.4.7",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.1"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.830.0",
    "@aws-sdk/credential-provider-ini": "^3.830.0",
    "@aws-sdk/node-http-handler": "^3.370.0",
    "@hookform/resolvers": "^3.9.1",
    "@lancedb/lancedb": "^0.12.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "express": "^4.21.1",
    "immer": "^10.1.1",
    "langchain": "^0.3.6",
    "lucide-react": "^0.454.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-resizable-panels": "^2.1.7",
    "react-window": "^1.8.10",
    "tailwind-merge": "^2.5.4",
    "uuid": "^10.0.0",
    "vaul": "^1.1.1",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "build": {
    "appId": "com.projectmaestro.app",
    "productName": "Project Maestro",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "public/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "public/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "public/icon.png"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
